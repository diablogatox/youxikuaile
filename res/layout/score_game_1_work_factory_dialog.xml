<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="425dp"
    android:layout_height="280dp" >

    <TextView
        android:id="@+id/hayou_score_2_game_dialog_top"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/score_game_2_dialog_title"
        android:gravity="center"
        android:padding="10dp"
        android:paddingRight="5dp"
        android:textColor="#ffffff"
        android:textSize="20sp" />

    <View
        android:id="@+id/hayou_score_2_game_dialog_bottom"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/score_dialog_bottom"
        android:padding="10dp" />

    <LinearLayout
        android:id="@+id/hayou_score_2_content_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/hayou_score_2_game_dialog_bottom"
        android:layout_below="@id/hayou_score_2_game_dialog_top"
        android:layout_centerHorizontal="true"
        android:background="#ffffff"
        android:orientation="horizontal" >
    </LinearLayout>

</RelativeLayout>